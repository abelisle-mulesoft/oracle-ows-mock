	<xs:simpleType name="VectorDirectionTypeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="North"/>
			<xs:enumeration value="NorthEast"/>
			<xs:enumeration value="East"/>
			<xs:enumeration value="SouthEast"/>
			<xs:enumeration value="South"/>
			<xs:enumeration value="SouthWest"/>
			<xs:enumeration value="West"/>
			<xs:enumeration value="NorthWest"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="VectorDistanceType">
		<xs:simpleContent>
			<xs:extension base="xs:double">
				<xs:attribute name="distanceUnit" type="avail:DistanceUnitTypeType" use="required"/>
				<xs:attribute name="otherDistanceUnit" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	
