					<xs:element name="TraceDate" type="xs:date" minOccurs="0"/>
					<xs:element name="TraceTime" type="xs:time" minOccurs="0"/>
					<xs:element name="ResolvedOn" type="xs:date" minOccurs="0"/>
					<xs:element name="ResolvedBy" type="xs:string" minOccurs="0"/>
				</xs:sequence>
				<xs:attribute name="department" type="xs:string"/>
				<xs:attribute name="resolved" type="xs:boolean"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="ReservationTracesListType">
